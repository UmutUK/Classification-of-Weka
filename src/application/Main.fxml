<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <VBox fx:id="Vbox" prefHeight="500.0" prefWidth="700.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#FileBrowser" text="File Browse" />
                  <Button mnemonicParsing="false" onAction="#Bayes" text="Bayes" />
                  <Button mnemonicParsing="false" onAction="#OneR" text="OneR" />
                  <Button mnemonicParsing="false" onAction="#DecisionTable" text="DecisionTable" />
                  <Button mnemonicParsing="false" onAction="#compare" text="Compare" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="300.0" prefWidth="500.0">
               <children>
                  <TextArea fx:id="txa" prefHeight="300.0" prefWidth="564.0" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
